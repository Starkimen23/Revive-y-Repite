{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    { "source": "/static/:path*",    "destination": "https://script.google.com/static/:path*" },
    { "source": "/wardeninit",       "destination": "https://script.google.com/wardeninit" },
    { "source": "/wardenlog",        "destination": "https://script.google.com/wardenlog" },

    { "source": "/",                 "destination": "https://script.google.com/macros/s/AKfycbytR7BEpQr4dsyKhtkwC6ShDNPRB0lzNyfPx3uV2vAIN_wylr-cz1vwUpl2zwDcfLPRWw/exec" },
    { "source": "/:path*",           "destination": "https://script.google.com/macros/s/AKfycbytR7BEpQr4dsyKhtkwC6ShDNPRB0lzNyfPx3uV2vAIN_wylr-cz1vwUpl2zwDcfLPRWw/exec" }
  ],
  "headers": [
    {
      "source": "/static/:path*",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
